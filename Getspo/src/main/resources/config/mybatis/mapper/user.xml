<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="u">

<!-- 회원가입 -->
	<insert id="insert_user" parameterType="user">
		insert into getspo_user values(seq_user_idx.nextVal, #{name}, #{pwd}, #{birth}, #{tel}, #{email}, #{addr}, #{addrcode}, #{addrdetail})
	</insert>
	
<!-- 이메일 중복확인 -->

<select id="select_email" resultType="String" parameterType="String">
select * from getspo_user where email = #{email}
</select>
</mapper>












